package com.example;

public class Main {

	public static void main(String[] args) {
		int arr[];
	    arr=new int[10];
	    for(int i=0;i<arr.length;i++)
	    {
	    	arr[i]=i+1;
	    }

	    for(int i=0;i<arr.length;i++)
	    {
	    	System.out.println(arr[i]);
	    }
	    arr=new int[11];
	    for(int i=0;i<arr.length;i++)
	    {
	    	System.out.println(arr[i]);
	    }
	}

}