package com.example;

public class MyDate {
	
	private int day;
	private int month;
	private int year;
	
	public void createDate(int d,int m,int y)
	{
		day=d;
		month=m;
		year=y;
	}
	
	public String getMyDate()
	{
		return "Date is: "+day+"/"+month+"/"+year;
	}
	public static void main(String[] args) {
        /*
         * ToDo todo; todo=new ToDo(); todo.isCompleted=true; todo.todoName="Compile";
         * todo.assigneeName="John Doe"; System.out.println("ToDo Name: "+todo.todoName
         * + "ToDo Assignee Name:  "+todo.assigneeName +
         * "ToDo Is Completed: "+todo.isCompleted);
         */
        
		/*
		 * Elevator elevator=new Elevator();
		 * 
		 * elevator.goUp(100); elevator.goDown(5);
		 */

    	MyDate mydate=new MyDate();
    	mydate.createDate(26,12,1996);
    	System.out.println(mydate.getMyDate());

    }
}
