package com.fly;

public class SeaPlane implements Flyer {
	
	@Override
	public void takeOff() {
		System.out.println("Sea Plane takingoff");
		
	}

	@Override
	public void land() {
		System.out.println("Sea Plane landing");
		
	}

	@Override
	public void fly() {
		System.out.println("Sea Plane flying");
		
	}

	@Override
	public void getDetails() {
		// TODO Auto-generated method stub
		super.getDetails();
		System.out.println("hello i am Sea Plane");
	}

}
